<AnchorPane fx:id="anchorpane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: skyblue;" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo4.BookingController">
   <children>
      <ComboBox fx:id="comboboxSlect" layoutX="177.0" layoutY="73.0" prefWidth="150.0" />
      <ComboBox fx:id="comboboxVehicle" layoutX="177.0" layoutY="120.0" prefWidth="150.0" />
      <Label fx:id="lblBookingsystem" layoutX="213.0" layoutY="28.0" text="Booking System">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblCustomer" layoutX="37.0" layoutY="77.0" text="Select Customer:" />
      <Label fx:id="lblvehicle" layoutX="37.0" layoutY="124.0" text="Select Vehicle:" />
      <Label fx:id="lblrental" layoutX="37.0" layoutY="183.0" text="Rental StartDate:" />
      <Label fx:id="lblEnd" layoutX="37.0" layoutY="227.0" text="Rental EndDate:" />
      <DatePicker fx:id="dateRental" layoutX="161.0" layoutY="179.0" />
      <DatePicker fx:id="dateEndDate" layoutX="162.0" layoutY="223.0" />
      <Button layoutX="198.0" layoutY="360.0" mnemonicParsing="false" onAction="#modify" text="Modify" />
      <Button layoutX="386.0" layoutY="360.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Label fx:id="lblBookingSummary" layoutX="37.0" layoutY="276.0" text="Booking Summary:" />
      <TextArea fx:id="textarea" layoutX="162.0" layoutY="265.0" prefHeight="82.0" prefWidth="278.0" />
      <Label fx:id="lblRentalDuration" layoutX="412.0" layoutY="77.0" text="Rental Duration" />
      <Button layoutX="469.0" layoutY="360.0" mnemonicParsing="false" onAction="#Exit" text="Exit" />
      <Button layoutX="285.0" layoutY="360.0" mnemonicParsing="false" onAction="#confirmBooking" text="Book" />
   </children>
</AnchorPane>
